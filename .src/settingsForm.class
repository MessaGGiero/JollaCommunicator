' Gambas class file

Public Sub Run() As Boolean
  
  Configuration.ReadConfig()
  fillAreaCode()
  
    If Configuration.hostName = "undefined" Then
      cmbAreaCode.Index = cmbAreaCode.find("Italy(+39)")
    Else 
      textHostName.Text = Configuration.hostName
      textPassword.Text = Configuration.pwdNemo 
      cmbAreaCode.index = cmbAreaCode.find(Configuration.areaCode)
    Endif
    Return Not Me.ShowModal()

End

Public Sub btnOK_Click()
  
  Dim lHostname As String
  Dim lPasswd As String
  Dim lAreaCode As String

  lHostname = Trim(textHostName.Text)
  lPasswd = Trim(textPassword.Text)
  lAreaCode = Trim(cmbAreaCode.text)
  
  If IsNull(lHostname) Then
    
    Message.Error("Enter IP address or hostname")
    Return
  Endif
  
  If IsNull(lPasswd) Then
    
    Message.Error("Enter Password for user Nemo")
    Return
    
  Endif
  
  If IsNull(lAreaCode) Then
    
    Message.Error("Enter the correct Area Code")
    Return
    
  Endif
   Configuration.SaveConfig(lHostname, lPasswd, lAreaCode)
  
  Me.Close(True)

End

Public Sub btnCancel_Click()

  Me.Close(True)

End



